{
  "permissions": {
    "file_system": {
      "read": true,
      "write": true,
      "create": true,
      "delete": true,
      "move": true
    },
    "bash": {
      "enabled": true,
      "commands": {
        "allow_all": true
      }
    },
    "web": {
      "enabled": true,
      "domains": ["*"]
    }
  },
  "auto_approve": {
    "file_edits": true,
    "file_creates": true,
    "bash_commands": true,
    "web_requests": true
  },
  "context": {
    "max_tokens": 200000,
    "include_file_contents": true,
    "auto_refresh": true
  },
  "development": {
    "test_runner": "pytest",
    "python_path": "venv/bin/python",
    "auto_lint": false,
    "auto_format": false
  },
  "project": {
    "type": "python",
    "name": "Agent Otel for Lake Merritt Eval Packs",
    "description": "AI evaluation workbench eval pack components",
    "main_branch": "main",
    "working_branch": "feature/eval-packs-june-28"
  }
}